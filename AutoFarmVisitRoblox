--// SAFE AUTO FARM (CLIENT ONLY)
local TeleportService = game:GetService("TeleportService")
local Players = game:GetService("Players")
local UIS = game:GetService("UserInputService")

local lp = Players.LocalPlayer
local GAME_ID = 90746724868686

local visits = 0
local running = true
local teleporting = false

print("üöÄ –ê–í–¢–û–§–ê–†–ú –ó–ê–ü–£–©–ï–ù")

local function waitChar()
	if lp.Character and lp.Character:FindFirstChild("Humanoid") then
		return lp.Character
	end
	return lp.CharacterAdded:Wait()
end

local function quickAct()
	local char = waitChar()
	local hum = char:FindFirstChild("Humanoid")
	if not hum then return end

	hum:Move(Vector3.new(1,0,0), false)
	task.wait(0.05)
	hum:Move(Vector3.new(-1,0,0), false)
	task.wait(0.05)
	hum.Jump = true
end

local function safeTeleport()
	if teleporting then return end
	teleporting = true

	visits += 1
	quickAct()

	task.wait(0.3)
	TeleportService:Teleport(GAME_ID, lp)
end

task.spawn(function()
	task.wait(2)

	while running and visits < 10000 do
		waitChar()
		safeTeleport()

		task.wait(8) -- üîí –∞–Ω—Ç–∏-—Ñ–ª—É–¥

		if visits % 5 == 0 then
			print("‚ö° –í–ò–ó–ò–¢–û–í:", visits)
			print("üéØ JOB:", game.JobId)
		end
	end
end)

UIS.InputBegan:Connect(function(input, gp)
	if gp then return end

	if input.KeyCode == Enum.KeyCode.P then
		running = false
		print("üõë –û–°–¢–ê–ù–û–í–õ–ï–ù–û")
	elseif input.KeyCode == Enum.KeyCode.R then
		visits = 0
		print("üîÑ –°–ë–†–û–°")
	end
end)

print("‚úÖ –ì–û–¢–û–í–û (CLIENT SAFE)")
